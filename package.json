{
  "name": "web-ai",
  "version": "1.0.0",
  "description": "ai for web",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon server.js",
    "skl": "java -jar ./packages/sikulixide-2.0.5.jar",
    "debug": "DEBUG=* NODE_ENV=development nodemon server.js",
    "prod": "(pm2 delete server || true) && pm2 start server.js",
    "baseContainer": "docker run --rm --user root -it --shm-size=512m -p 6901:6901 -p 3000:3000 -e VNC_PW=admin --name base-ai-container kasmweb/ubuntu-jammy-desktop:1.14.0-rolling",
    "createZlImg": "(docker rmi zl-ai-img || true) && cd ./ai-img && docker build -t zl-ai-img .",
    "zlContainer": "docker run --rm --user root -it --shm-size=512m -p 6901:6901 -p 3000:3000 -e VNC_PW=admin --name zl-ai-container zl-ai-img",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangliu/web-ai.git"
  },
  "keywords": [
    "web",
    "ai"
  ],
  "author": "zhangliu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zhangliu/web-ai/issues"
  },
  "homepage": "https://github.com/zhangliu/web-ai#readme",
  "dependencies": {
    "@koa/bodyparser": "^5.0.0",
    "electron": "11.0.0",
    "koa": "^2.14.2",
    "koa-router": "^12.0.0",
    "koa2-cors": "^2.0.6",
    "moment": "^2.29.4",
    "nodemon": "^3.0.1",
    "openai": "^4.0.1",
    "pm2": "^5.3.0",
    "puppeteer": "^21.3.8"
  }
}
